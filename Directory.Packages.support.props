<?xml version="1.0" encoding="utf-8"?>
<Project>
    <ItemGroup
        Condition="'$(TargetFramework)' == 'netstandard2.0' or '$(TargetFramework)' == 'netstandard2.1'">
        <PackageVersion Update="Microsoft.AspNetCore.TestHost" Version="3.1.32" />
    </ItemGroup>
    <ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
        <PackageVersion Update="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.24" />
        <PackageVersion Update="Microsoft.AspNetCore.Components.WebAssembly.DevServer"
            Version="6.0.24" />
        <PackageVersion Update="Microsoft.AspNetCore.TestHost" Version="6.0.24" />
    </ItemGroup>
    <ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
        <PackageVersion Update="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.11" />
        <PackageVersion Update="Microsoft.AspNetCore.Components.WebAssembly.DevServer"
            Version="8.0.11" />
        <PackageVersion Update="Microsoft.AspNetCore.TestHost" Version="8.0.11" />
    </ItemGroup>
</Project>